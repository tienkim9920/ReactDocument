enu\"><a className={kythuat} href=\"#kythuat\">8. Kỹ thuật cơ bản (Array)🚀</a></li>\r\n                </ul>\r\n            </div>\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default DotNet;","import React, { useEffect, useState } from 'react';\r\nimport PropTypes from 'prop-types';\r\nimport './Menu.css'\r\nimport { useSelector } from 'react-redux';\r\nimport { Link, useLocation } from 'react-router-dom';\r\n\r\nMenu.propTypes = {\r\n\r\n};\r\n\r\nfunction Menu(props) {\r\n\r\n\r\n    const isCheck = useSelector(state => state.Check.isCheck)\r\n\r\n\r\n    const [Active, SetActive] = useState({\r\n        category: 'dotnet',\r\n        key: 'install'\r\n    })\r\n\r\n    const handlerDotNet = (category, key) => {\r\n\r\n        SetActive({\r\n            category: category,\r\n            key: key\r\n        })\r\n\r\n    }\r\n\r\n    // Cái này dùng để lấy url mà mỗi khi đổi component\r\n    // Và nó sẽ chạy lại useEffect\r\n    let location = useLocation();\r\n\r\n    useEffect(() => {\r\n\r\n        const url = location.pathname\r\n\r\n        if (url.indexOf('dotnet/document') > 1){\r\n            SetActive({\r\n                category: 'dotnet',\r\n                key: 'docs'\r\n            })\r\n        }\r\n\r\n    }, [location])\r\n\r\n    return (\r\n        <div  className=\"menu\">\r\n            <li className=\"li_menu\" data-toggle=\"collapse\" href=\"#menu_dotnet_temp\" role=\"button\" aria-expanded=\"false\" aria-controls=\"menu_dotnet_temp\">\r\n                <div className=\"d-flex justify-content-between pr-2\">\r\n                    <a className={Active.category === 'dotnet' ? 'a_menu a_active' : 'a_menu'}\r\n                         >\r\n                        Khóa học C# cơ bản\r\n                    </a>\r\n                    <div className=\"menu_list\"></div>\r\n                </div>\r\n            </li>\r\n            <ul className=\"collapse menu_dotnet show\" id=\"menu_dotnet_temp\">\r\n                <Link\r\n                 